    getRandomRadio: functionL:(max)=>{
        return Math.floor(math.random()* max);
    }
]
//change station function

changeStation: function() {
let getRandomRadio = Math.floor(Math.random() * radio.stations.length);
let randomSong = Math.floor(Math.random() * radio.stations.[getRandomRadio.songs].songs.length;

console.log("Now Playing: ") + radio.stations[getRandomRadio].songs[randomSong].title + " by " + radio.stations[getRandomRadio].songs[randomSong].artist + " on the "+ radio.stations[getRandomRadio].name + "station!");
}

radio.changeStation(); 
